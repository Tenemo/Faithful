                        p.text-center.contact-link(data-i18n="contact.facebook") wiernypiespl
                .col-sm-2
                    a(href='https://www.instagram.com/wiernypies.pl' target='_blank').fa.fa-instagram.fa-3x.strong
                        p.text-center.contact-link(data-i18n="contact.instagram") wiernypiespl
                .col-sm-2
                    a(href='mailto:kontakt@wiernypies.pl').fa.fa-envelope-o.fa-3x.strong
                        p.text-center.contact-link(data-i18n="contact.email") kontakt@wiernypies.pl
            .row
