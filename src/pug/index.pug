    //- CONTACT ===========================
    section#contact.fullscreen(data-section-name='contact',data-i18n="[data-section-translation]main.contact")
        //- CONTACT FORM
        #contactForm.modal.fade.text-left(tabindex='-1', role='dialog', aria-labelledby='contactFormLabel', aria-hidden='true')
            .modal-dialog
                .modal-content
                    .modal-header
                        button.close(type='button', data-dismiss='modal', aria-hidden='true') &times;
                        h3#contactFormLabel(data-i18n="contact.form.main") Formularz kontaktowy
                    .modal-body
                        form.form-horizontal.col-sm-12(id="contact-form" method="post" action="contact.php" role="form")
                            .text-muted.pull-right(data-i18n="contact.form.required" style='margin-bottom: 0px; font-style: italic; font-size: 12px;') * te pola są wymagane
                            .form-group
                                label(for='form_name' data-i18n="contact.form.firstname") Imię
                                |  *
                                input#form_name.form-control(
                                    type='text',
                                    name='name',
                                    data-i18n="[placeholder]contact.form.firstnamePlease;[data-error]contact.form.firstnameError"
                                    placeholder='Proszę podać swoje imię',
                                    required='required',
                                    data-error='Imię jest wymagane.'
                                )
                                .help-block.with-errors
                            .form-group
                                label(for='form_lastname' data-i18n="contact.form.lastname") Nazwisko
                                |  *
                                input#form_lastname.form-control(
                                    type='text',
                                    name='surname',
                                    data-i18n="[placeholder]contact.form.lastnamePlease;[data-error]contact.form.lastnameError"
                                    placeholder='Proszę podać swoje nazwisko',
                                    required='required',
                                    data-error='Nazwisko jest wymagane.'
                                )
                                .help-block.with-errors
                            .form-group
                                label(for='form_email' data-i18n="contact.form.email") E-mail
                                |  *
                                input#form_email.form-control(
                                    type='email',
                                    name='email',
                                    data-i18n="[placeholder]contact.form.emailPlease;[data-error]contact.form.emailError"
                                    placeholder='Proszę podać swój adres e-mail.',
                                    required='required',
                                    data-error='E-mail jest wymagany.'
                                )
                                .help-block.with-errors
                            .form-group
                                label(for='form_phone' data-i18n="contact.form.phone") Telefon
                                input#form_phone.form-control(
                                    type='tel',
                                    name='phone',
                                    data-i18n="[placeholder]contact.form.phonePlease"
                                    placeholder='Podaj numer telefonu'
                                )
                                .help-block.with-errors
                            .form-group(style='margin-bottom: 10px;')
                                label(for='form_message' data-i18n="contact.form.message") Wiadomość
                                |  *
                                textarea#form_message.form-control(
                                    name='message',
                                    data-i18n="[placeholder]contact.form.messagePlease;[data-error]contact.form.messageError"
                                    placeholder='Treść wiadomości',
                                    rows='4',
                                    required='required',
                                    data-error='Treść wiadomości jest wymagana.'
                                )
                                .help-block.with-errors
                            .form-group(style='margin-bottom: 5px;')
                                input.btn.btn-primary.btn-send(type='submit', value='Wyślij wiadomość', data-i18n="[value]contact.form.buttonSend")
                                button.btn.pull-right(data-dismiss='modal', aria-hidden='true' data-i18n="contact.form.buttonCancel") Cancel
                            .form-group
                            .messages
                        //- fix for modal-body shortening span(style='display: inline-block;') &nbsp;
                        div(style="clear: both;")
                    .modal-footer
                        .messages
        .container-fluid
            .row
                .col-xs-12.text-center
                    h2.section-heading(data-i18n="contact.getInTouch") Skontaktuj się z nami
